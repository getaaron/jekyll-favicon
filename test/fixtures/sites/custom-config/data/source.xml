<?xml version='1.0' encoding='utf-8'?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src='images/harcoded/smalltile.png'/>
      <square150x150logo src='images/harcoded/mediumtile.png'/>
      <wide310x150logo src='images/harcoded/widetile.png'/>
      <square310x310logo src='images/harcoded/largetile.png'/>
      <TileColor>#009900</TileColor>
    </tile>
    <notification>
      <polling-uri  src='notifications/1.xml'/>
      <polling-uri2 src='notifications/2.xml'/>
      <polling-uri3 src='notifications/3.xml'/>
      <frequency>30</frequency>
      <cycle>1</cycle>
    </notification>
  </msapplication>
</browserconfig>
