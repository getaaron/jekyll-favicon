<!-- Safari -->
<%- favicon_path = File.join prepend_path, Favicon.config['path'] -%>
<%- Favicon.config['icons']['safari']['targets'].each do |target| -%>
  <link rel="apple-touch-icon" sizes="<%= target %>" href="<%= File.join favicon_path, target %>">
<%- end -%>
<% if  Favicon.config['source'].svg? %>
<link rel="mask-icon" color="<%= Favicon.config['icons']['safari']['pinned-tab']['color'] %>" href="<%= File.join favicon_path, 'safari-pinned-tab.svg' %>">
<% end %>
