<!-- Chrome -->
<%- Favicon.config['icons']['chrome']['targets'].each do |size| -%>
  <link rel="icon" sizes="<%= size %>" href="<%= File.join prepend_path, Favicon.config['path'], size %>">
<%- end -%>
<link rel="manifest" href="<%= File.join prepend_path, Favicon.config['webmanifest']['target'] %>">
